<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee 
                             https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
         version="5.0">

    <servlet>
        <servlet-name>ChargeServlet</servlet-name>
        <servlet-class>controller.charge.ChargeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ChargeServlet</servlet-name>
        <url-pattern>/charge</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetCharge</servlet-name>
        <servlet-class>controller.charge.GetCharge</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetCharge</servlet-name>
        <url-pattern>/getCharge</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MouvementChargeServlet</servlet-name>
        <servlet-class>controller.mouvement.MouvementChargeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MouvementChargeServlet</servlet-name>
        <url-pattern>/mouvementCharge</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MouvementProduitServlet</servlet-name>
        <servlet-class>controller.mouvement.MouvementProduitServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MouvementProduitServlet</servlet-name>
        <url-pattern>/mouvementProduit</url-pattern>
    </servlet-mapping>

</web-app>
